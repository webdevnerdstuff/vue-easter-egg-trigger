		"gh-pages": "^5.0.0",
		"husky": "^8.0.0",
		"lint-staged": "^13.2.3",
		"prettier": "^3.0.0",
		"rollup": "^3.26.3",
		"rollup-plugin-typescript2": "^0.35.0",
		"typescript": "^5.1.6",
		"vite": "^4.4.5",
		"vite-plugin-dts": "^3.3.1",
		"vite-plugin-eslint": "^1.8.1",
		"vue": "^3.3.4",
